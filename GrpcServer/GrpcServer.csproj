  <ItemGroup>
    <None Remove="10mbanswerset.txt" />
    <None Remove="Protos\customers.proto" />
    <None Remove="Protos\interview.proto" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="10mbanswerset.txt" />
